{"ast":null,"code":"export default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      date: \"\",\n      event: \"\",\n      state: \"\",\n      remark: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      headerBg: 'headerBg'\n    };\n  },\n  created() {},\n  methods: {\n    load() {\n      this.request.get(\"/schedule/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          date: this.date,\n          event: this.event,\n          state: this.state,\n          remark: this.remark\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.total;\n      });\n    },\n    save() {\n      this.request.post(\"/schedule\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    handadd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"/schedule/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    reset() {\n      this.date = \"\";\n      this.event = \"\";\n      this.state = \"\";\n      this.remark = \"\";\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","date","event","state","remark","form","dialogFormVisible","multipleSelection","headerBg","created","methods","load","request","get","params","then","res","console","log","save","post","$message","success","error","cancel","handadd","handleEdit","row","JSON","parse","stringify","del","id","delete","handleSelectionChange","val","reset","handleSizeChange","handleCurrentChange"],"sources":["src/views/User.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div style=\"margin-bottom: 30px\">\r\n          <el-breadcrumb separator=\"/\">\r\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item><a href=\"/\">日程查询</a></el-breadcrumb-item>\r\n          </el-breadcrumb>\r\n        </div>\r\n        <div style=\"padding: 10px 0\">\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入事务\" suffix-icon=\"el-icon-message-solid\" class=\"ml-5\" v-model=\"event\"></el-input>\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入状态\" suffix-icon=\"el-icon-s-order\" class=\"ml-5\" v-model=\"state\"></el-input>\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入备注\" suffix-icon=\"el-icon-edit\" class=\"ml-5\" v-model=\"remark\"></el-input>\r\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n          <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n\r\n        <div style=\"margin: 10px 0\">\r\n          <el-button type=\"primary\" @click=\"handadd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n          <el-button type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          <el-button type=