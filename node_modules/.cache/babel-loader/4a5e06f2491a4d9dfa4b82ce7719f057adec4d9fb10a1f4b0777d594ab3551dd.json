{"ast":null,"code":"export default {\n  name: \"Friend\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      friendName: \"\",\n      nickname: \"\",\n      telephone: \"\",\n      email: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      headerBg: 'headerBg',\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/friend/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          friendName: this.friendName,\n          nickname: this.nickname,\n          telephone: this.telephone,\n          email: this.email,\n          username: this.user.username\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.total;\n        this.checkFriendRequests();\n      });\n    },\n    checkFriendRequests() {\n      this.request.get(\"/friend/\" + this.user.username).then(res => {\n        if (res) {\n          this.$confirm('您有新的好友申请，来自用户：' + res.username + ' 是否通过？', '提示', {\n            confirmButtonText: '同意',\n            cancelButtonText: '拒绝',\n            type: 'info'\n          }).then(() => {\n            this.handleFriendRequests(true);\n          }).catch(() => {\n            this.handleFriendRequests(false);\n          });\n        }\n      });\n    },\n    handleFriendRequests(accepted) {\n      if (accepted) {\n        this.request.post(`/friend/agree/${this.user.username}/${this.friendName}`).then(res => {});\n        this.$message.success('已同意好友申请');\n      } else {\n        this.request.post(`/friend/refuse/${this.user.username}/${this.friendName}`).then(res => {});\n        this.$message.info('已拒绝好友申请');\n      }\n    },\n    save() {\n      this.form.username = this.user.username;\n      this.request.post(\"/friend\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"好友申请已发送\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    handadd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    del(friendName) {\n      this.request.delete(`/friend/delete/${friendName}/${this.user.username}`).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    reset() {\n      this.friendName = \"\";\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","friendName","nickname","telephone","email","form","dialogFormVisible","multipleSelection","headerBg","user","localStorage","getItem","JSON","parse","created","load","methods","request","get","params","username","then","res","console","log","checkFriendRequests","$confirm","confirmButtonText","cancelButtonText","type","handleFriendRequests","catch","accepted","post","$message","success","info","save","code","error","msg","cancel","handadd","handleEdit","row","stringify","del","delete","handleSelectionChange","val","reset","handleSizeChange","handleCurrentChange"],"sources":["src/views/Friend.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入用户名\" suffix-icon=\"el-icon-user\" class=\"ml-5\" v-model=\"friendName\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handadd\">添加好友 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column prop=\"friendName\" label=\"用户名\" width=\"220\"></el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"220\"></el-table-column>\r\n      <el-table-column prop=\"telephone\" label=\"电话\" width=\"220\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\" width=\"220\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template #default=\"scope\">\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"确定删除该好友吗？\"\r\n              @confirm=\"del(scope.row.friendName)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"添加好友\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form label-width=\"110px\" size=\"small\">\r\n        <el-form-item label=\"请输入用户名\" >\r\n          <el-input v-model=\"form.friendName\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default{\r\n  name: \"Friend\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      friendName: \"\",\r\n      nickname: \"\",\r\n      telephone: \"\",\r\n      email: \"\",\r\n      form:{},\r\n      dialogFormVisible: false,\r\n      multipleSelection:[],\r\n      headerBg:'headerBg',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n    }\r\n  },\r\n  created(){\r\n    this.load();\r\n  },\r\n  methods:{\r\n    load(){\r\n      this.request.get(\"/friend/page\",{\r\n        params:{\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          friendName: this.friendName,\r\n          nickname: this.nickname,\r\n          telephone: this.telephone,\r\n          email: this.email,\r\n          username: this.user.username\r\n        }\r\n      }).then(res =>{\r\n        console.log(res)\r\n        this.tableData = res.data\r\n        this.total = res.total\r\n        this.checkFriendRequests()\r\n      })\r\n    },\r\n    checkFriendRequests() {\r\n      this.request.get(\"/friend/\" + this.user.username).then(res =>{\r\n        if(res){\r\n          this.$confirm('您有新的好友申请，来自用户：' + res.username + ' 是否通过？', '提示', {\r\n            confirmButtonText: '同意',\r\n            cancelButtonText: '拒绝',\r\n            type: 'info'\r\n          }).then(() => {\r\n            this.handleFriendRequests(true);\r\n          }).catch(() => {\r\n            this.handleFriendRequests(false);\r\n          });\r\n        }\r\n      })\r\n    },\r\n    handleFriendRequests(accepted) {\r\n      if (accepted) {\r\n        this.request.post(`/friend/agree/${this.user.username}/${this.friendName}`).then(res =>{\r\n\r\n        })\r\n        this.$message.success('已同意好友申请');\r\n      } else {\r\n        this.request.post(`/friend/refuse/${this.user.username}/${this.friendName}`).then(res =>{\r\n\r\n        })\r\n        this.$message.info('已拒绝好友申请');\r\n      }\r\n    },\r\n    save(){\r\n      this.form.username = this.user.username\r\n      this.request.post(\"/friend\",this.form).then(res =>{\r\n        if(res.code === '200'){\r\n          this.$message.success(\"好友申请已发送\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    cancel(){\r\n      this.dialogFormVisible=false\r\n      this.load()\r\n    },\r\n    handadd(){\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    del(friendName){\r\n      this.request.delete(`/friend/delete/${friendName}/${this.user.username}`).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"删除成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    reset(){\r\n      this.friendName = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize){\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg{\r\n  background: #eee!important;\r\n}\r\n</style>"],"mappings":"AA8DA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,UAAA;MACAC,QAAA;MACAC,SAAA;MACAC,KAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,QAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACApB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,UAAA,OAAAA,UAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,SAAA,OAAAA,SAAA;UACAC,KAAA,OAAAA,KAAA;UACAgB,QAAA,OAAAX,IAAA,CAAAW;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAzB,SAAA,GAAAyB,GAAA,CAAA1B,IAAA;QACA,KAAAE,KAAA,GAAAwB,GAAA,CAAAxB,KAAA;QACA,KAAA2B,mBAAA;MACA;IACA;IACAA,oBAAA;MACA,KAAAR,OAAA,CAAAC,GAAA,mBAAAT,IAAA,CAAAW,QAAA,EAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAI,QAAA,oBAAAJ,GAAA,CAAAF,QAAA;YACAO,iBAAA;YACAC,gBAAA;YACAC,IAAA;UACA,GAAAR,IAAA;YACA,KAAAS,oBAAA;UACA,GAAAC,KAAA;YACA,KAAAD,oBAAA;UACA;QACA;MACA;IACA;IACAA,qBAAAE,QAAA;MACA,IAAAA,QAAA;QACA,KAAAf,OAAA,CAAAgB,IAAA,uBAAAxB,IAAA,CAAAW,QAAA,SAAAnB,UAAA,IAAAoB,IAAA,CAAAC,GAAA,KAEA;QACA,KAAAY,QAAA,CAAAC,OAAA;MACA;QACA,KAAAlB,OAAA,CAAAgB,IAAA,wBAAAxB,IAAA,CAAAW,QAAA,SAAAnB,UAAA,IAAAoB,IAAA,CAAAC,GAAA,KAEA;QACA,KAAAY,QAAA,CAAAE,IAAA;MACA;IACA;IACAC,KAAA;MACA,KAAAhC,IAAA,CAAAe,QAAA,QAAAX,IAAA,CAAAW,QAAA;MACA,KAAAH,OAAA,CAAAgB,IAAA,iBAAA5B,IAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAgB,IAAA;UACA,KAAAJ,QAAA,CAAAC,OAAA;UACA,KAAA7B,iBAAA;UACA,KAAAS,IAAA;QACA;UACA,KAAAmB,QAAA,CAAAK,KAAA,CAAAjB,GAAA,CAAAkB,GAAA;QACA;MACA;IACA;IACAC,OAAA;MACA,KAAAnC,iBAAA;MACA,KAAAS,IAAA;IACA;IACA2B,QAAA;MACA,KAAApC,iBAAA;MACA,KAAAD,IAAA;IACA;IACAsC,WAAAC,GAAA;MACA,K