{"ast":null,"code":"export default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      date: \"\",\n      event: \"\",\n      state: \"\",\n      remark: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      headerBg: 'headerBg'\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/schedule/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          date: this.date,\n          event: this.event,\n          state: this.state,\n          remark: this.remark\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.map(item => {\n          // 将日期转换为 Date 对象\n          let date = new Date(item.date);\n          // 推后一天\n          date.setDate(date.getDate() + 1);\n          // 转换为 yyyy-MM-dd 格式\n          item.date = date.toISOString().split('T')[0];\n          return item;\n        });\n        this.total = res.total;\n      });\n    },\n    save() {\n      this.request.post(\"/schedule\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    handadd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"/schedule/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.request.post(\"/schedule/del/batch\" + ids).then(res => {\n        if (res) {\n          this.$message.success(\"批量删除成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    reset() {\n      this.date = \"\";\n      this.event = \"\";\n      this.state = \"\";\n      this.remark = \"\";\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","date","event","state","remark","form","dialogFormVisible","multipleSelection","headerBg","created","load","methods","request","get","params","then","res","console","log","map","item","Date","setDate","getDate","toISOString","split","save","post","$message","success","error","cancel","handadd","handleEdit","row","JSON","parse","stringify","del","id","delete","handleSelectionChange","val","delBatch","ids","v","reset","handleSizeChange","handleCurrentChange"],"sources":["src/views/User.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div style=\"margin-bottom: 30px\">\r\n          <el-breadcrumb separator=\"/\">\r\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item><a href=\"/\">日程管理</a></el-breadcrumb-item>\r\n          </el-breadcrumb>\r\n        </div>\r\n        <div style=\"padding: 10px 0\">\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入事务\" suffix-icon=\"el-icon-message-solid\" class=\"ml-5\" v-model=\"event\"></el-input>\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入状态\" suffix-icon=\"el-icon-s-order\" class=\"ml-5\" v-model=\"state\"></el-input>\r\n          <el-input style=\"width: 200px\" placeholder=\"请输入备注\" suffix-icon=\"el-icon-edit\" class=\"ml-5\" v-model=\"remark\"></el-input>\r\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n          <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n\r\n        <div style=\"margin: 10px 0\">\r\n          <el-button type=\"primary\" @click=\"handadd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='取消'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"确定批量删除吗？\"\r\n              @confirm=\"delBatch\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </div>\r\n\r\n       \r\n        <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n          <el-table-column type=\"selection\"  width=\"55\"></el-table-column>\r\n          <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n          <el-table-column prop=\"date\" label=\"日期\" width=\"150\"></el-table-column>\r\n          <el-table-column prop=\"event\" label=\"事务\" width=\"150\"></el-table-column>\r\n          <el-table-column prop=\"state\" label=\"状态\" width=\"150\"></el-table-column>\r\n          <el-table-column prop=\"remark\" label=\"备注\"></el-table-column>\r\n          <el-table-column label=\"操作\">\r\n            <template #default=\"scope\">\r\n              <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n              <el-popconfirm\r\n                  class=\"ml-5\"\r\n                  confirm-button-text='确定'\r\n                  cancel-button-text='我再想想'\r\n                  icon=\"el-icon-info\"\r\n                  icon-color=\"red\"\r\n                  title=\"确定删除这段日程吗？\"\r\n                  @confirm=\"del(scope.row.id)\"\r\n              >          \r\n              <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n            </el-popconfirm>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n          <el-pagination\r\n              @size-change=\"handleSizeChange\"\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pageNum\"\r\n              :page-sizes=\"[2, 5, 10, 20]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, sizes, prev, pager, next, jumper\"\r\n              :total=\"total\">\r\n          </el-pagination>\r\n        </div>\r\n\r\n        <el-dialog title=\"日程\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n           <el-form label-width=\"80px\" size=\"small\">\r\n            <el-form-item label=\"日期\" >\r\n              <el-input v-model=\"form.date\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"事务\" >\r\n              <el-input v-model=\"form.event\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"状态\" >\r\n              <el-input v-model=\"form.state\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"备注\" >\r\n              <el-input v-model=\"form.remark\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n    \r\n    \r\n    \r\n  </el-form>\r\n  <div slot=\"footer\" class=\"dialog-footer\">\r\n    <!-- <el-button @click=\"dialogFormVisible = false\">取 消</el-button> -->\r\n    <el-button @click=\"cancel\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </div>\r\n</el-dialog>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default{\r\n    name: \"User\",\r\n    data(){\r\n        return{\r\n            tableData: [],\r\n            total: 0,\r\n            pageNum: 1,\r\n            pageSize: 2,\r\n            date:\"\",\r\n            event: \"\",\r\n            state: \"\",\r\n            remark: \"\",\r\n            form:{},\r\n            dialogFormVisible: false,\r\n            multipleSelection:[],\r\n            headerBg:'headerBg'\r\n        }\r\n    },\r\n    created(){\r\n      this.load();\r\n    },\r\n    methods:{\r\n      load(){\r\n      this.request.get(\"/schedule/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize: this.pageSize,\r\n          date: this.date,\r\n          event: this.event,\r\n          state: this.state,\r\n          remark: this.remark\r\n        }\r\n      }).then(res =>{\r\n            console.log(res)\r\n          this.tableData = res.data.map(item => {\r\n            // 将日期转换为 Date 对象\r\n            let date = new Date(item.date);\r\n            // 推后一天\r\n            date.setDate(date.getDate() + 1);\r\n            // 转换为 yyyy-MM-dd 格式\r\n            item.date = date.toISOString().split('T')[0];\r\n            return item;\r\n          });\r\n            this.total = res.total\r\n          })\r\n    },\r\n    save(){\r\n      this.request.post(\"/schedule\",this.form).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    cancel(){\r\n      this.dialogFormVisible=false\r\n      this.load()\r\n    },\r\n    handadd(){\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    del(id){\r\n      this.request.delete(\"/schedule/\" +id).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"删除成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      this.request.post(\"/schedule/del/batch\" + ids).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"批量删除成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.date=\"\"\r\n      this.event=\"\"\r\n      this.state=\"\"\r\n      this.remark=\"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize){\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    }\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg{\r\n  background: #eee!important;\r\n}\r\n</style>"],"mappings":"AAiGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,IAAA;MACAC,KAAA;MACAC,KAAA;MACAC,MAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAf,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,IAAA,OAAAA,IAAA;UACAC,KAAA,OAAAA,KAAA;UACAC,KAAA,OAAAA,KAAA;UACAC,MAAA,OAAAA;QACA;MACA,GAAAW,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAnB,SAAA,GAAAmB,GAAA,CAAApB,IAAA,CAAAuB,GAAA,CAAAC,IAAA;UACA;UACA,IAAAnB,IAAA,OAAAoB,IAAA,CAAAD,IAAA,CAAAnB,